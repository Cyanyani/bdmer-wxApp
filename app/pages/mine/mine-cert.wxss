.container{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items:center;
}

.image-box{
    border-radius:5px;
    border:2px dashed #F5F5F5;
    margin-top: 50px;
    padding-top: 5px;
    padding-bottom: 5px;
    width:100%;
    height:200px;
}

.text-no{
    border-radius:5px;
    padding: 5px 10px 5px 10px;
    background: #FF9900;
    color: #FFFFFF;
    margin-top: 15px;
}
.text-ok{
    border-radius:5px;
    padding: 5px 10px 5px 10px;
    background: #19be6b;
    color: #FFFFFF;
    margin-top: 15px;
}